    <ul class="social">
        <li><a href="#"><img src="/static/css/images/fb.png" /></a></li>
        <li><a href="#"><img src="/static/css/images/tw.png" /></a></li>
        <li><a href="#"><img src="/static/css/images/g+.png" /></a></li>
      </ul>
	</div>
  </footer>
{% block extra_body %}{% endblock %}
